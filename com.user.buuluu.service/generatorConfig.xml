<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration >

	<!-- 数据库驱动包位置:注意是要配置自己本地的地址 -->
     <classPathEntry location="C:\Users\BMI5\.m2\repository\mysql\mysql-connector-java\5.1.17\mysql-connector-java-5.1.17.jar" /> 
 
  <context id="context1" >
  <!-- 
  suppressAllComments:**阻止**生成注释，默认为false
suppressDate:**阻止**生成的注释包含时间戳，默认为false -->
 <!--  <commentGenerator>
    <property name="suppressAllComments" value="true"/>
    <property name="suppressDate" value="true"/>
   </commentGenerator> -->
   <!-- 数据库链接URL、用户名、密码 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/buuluu?useUnicode=true&amp;characterEncoding=utf-8" 
    userId="root" password="123456" />
    
     <!-- 生成实体类的包名和位置，这里配置将生成的实体类放在com.center.buuluu.model这个包下 -->
    <javaModelGenerator targetPackage="com.user.buuluu.model" targetProject="com.user.buuluu.model">
    
        <property name="enableSubPackages" value="true" />
        <!-- 如果字段为string且为null转换为空字符串 -->
        <property name="trimStrings" value="true" />
     </javaModelGenerator>
    
     <!-- 生成的SQL映射文件包名和位置，这里配置将生成的SQL映射文件放在com.center.buuluu.dao.xml这个包下 -->
    <sqlMapGenerator targetPackage="com.user.buuluu.dao.xml" targetProject="com.user.buuluu.service" >
    <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
    
    <!-- 生成DAO的包名和位置，这里配置将生成的dao类放在com.center.buuluu.dao.mapper这个包下 -->
    <javaClientGenerator targetPackage="com.user.buuluu.dao.mapper" targetProject="com.user.buuluu.service" type="XMLMAPPER" >
     <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
    
     <!-- 要生成那些表(更改tableName和domainObjectName就可以) -->
   <!--  <table schema="???" tableName="???" >
      <columnOverride column="???" property="???" /> -->
      <table tableName="app_user_bill_log" domainObjectName="AppUserBillLog" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" >
      <!-- 当id需要自增长的时候使用 -->
     <generatedKey column="id" sqlStatement="mysql" identity="true"/>
    </table> 
  </context>
</generatorConfiguration>

