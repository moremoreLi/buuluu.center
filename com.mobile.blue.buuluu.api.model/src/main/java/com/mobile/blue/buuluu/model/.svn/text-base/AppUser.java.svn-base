public class AppUser extends AbstractEntity{

	/**
	 * 
	 */
	private static final long serialVersionUID = 5154539163106825267L;
	
	private String nickName;//用户昵称
	private String icon;//用户头像
	private String pwd;//用户密码
	private String tel;//用户电话号码
	private String countryCode;//用户国家编号
	private String telApproveKey;//电话验证key
	private Integer telValueFlag;//电话验证状态
	private Long birthday;//生日日期
	private Integer sex;//性别
	private Integer pushStatus;//推送状态，0否，1是
	private Double log;//经度
	private Double lat;//纬度
	private String country;//国籍
	private Float flowCoins;//
	private Float makeFlow;//
	private Float userFlow;//
	private Integer credit;//用户信用值
	private Integer point;//用户积分
	
	
	public Integer getPoint() {
		return point;
	}
	public void setPoint(Integer point) {
		this.point = point;
	}
	public Float getFlowCoins() {
		return flowCoins;
	}
	public void setFlowCoins(Float flowCoins) {
		this.flowCoins = flowCoins;
	}
	public Float getMakeFlow() {
		return makeFlow;
	}
	public void setMakeFlow(Float makeFlow) {
		this.makeFlow = makeFlow;
	}
	public Float getUserFlow() {
		return userFlow;
	}
	public void setUserFlow(Float userFlow) {
		this.userFlow = userFlow;
	}
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public String getCountryCode() {
		return countryCode;
	}
	public void setCountryCode(String countryCode) {
		this.countryCode = countryCode;
	}
	public String getTelApproveKey() {
		return telApproveKey;
	}
	public void setTelApproveKey(String telApproveKey) {
		this.telApproveKey = telApproveKey;
	}
	
	public Integer getTelValueFlag() {
		return telValueFlag;
	}
	public void setTelValueFlag(Integer telValueFlag) {
		this.telValueFlag = telValueFlag;
	}
	public Long getBirthday() {
		return birthday;
	}
	public void setBirthday(Long birthday) {
		this.birthday = birthday;
	}
	public Integer getSex() {
		return sex;
	}
	public void setSex(Integer sex) {
		this.sex = sex;
	}
	public Integer getPushStatus() {
		return pushStatus;
	}
	public void setPushStatus(Integer pushStatus) {
		this.pushStatus = pushStatus;
	}
	public Double getLog() {
		return log;
	}
	public void setLog(Double log) {
		this.log = log;
	}
	public Double getLat() {
		return lat;
	}
	public void setLat(Double lat) {
		this.lat = lat;
	}
	public String getCountry() {
		return country;
	}
	public void setCountry(String country) {
		this.country = country;
	}
	public Integer getCredit() {
		return credit;
	}
	public void setCredit(Integer credit) {
		this.credit = credit;
	}
}
